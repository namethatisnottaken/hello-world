var total_amount_spend = 0;
var phone_price=100;
var accessory_price=10;
var tax_rate=0.1;

var bank_blc=1000;
var spending_threshold=200;

while (total_amount_spend < bank_blc ) {
	total_amount_spend=total_amount_spend+phone_price;
} if (total_amount_spend < spending_threshold) {
total_amount_spend=total_amount_spend+accessory_price;

}
//final amount + tax
var final_amount=0
final_amount=total_amount_spend+(total_amount_spend*tax_rate);
console.log (final_amount);
